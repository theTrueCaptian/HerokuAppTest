<html>
	<!-- Bootstrap core CSS -->
    <link href="/stylesheets/bootstrap.css" rel="stylesheet">
<body>
	<%-    adminSideMenu %>
	<div class="container">
	<p>
	<div class="panel panel-default">
		<div class="panel-heading">Log Out</div>
		<div class="panel-body">
		<form role="logout" action="/logout" method="get">
 			<button class="btn btn-primary" type="submit">Log Out</button>   
		</form>
		</div>
	</div>
	<p>
	<div class="panel panel-default">
		<div class="panel-heading"> Search for "a href" tags </div>
		<div class="panel-body">
			<form role="form" action="/adminsearch" method="post">
				Starting page <input type="link" class="form-control" placeholder="Link" required autofocus name="starting_pt">
				<!--Recursions <input type="text" class="form-control" placeholder="Region Name" required autofocus name="recursions">-->
				
				<button class="btn btn-lg btn-primary btn-block" type="submit">Search</button>   
				
				 
			</form>
		</div>
	</div>
	<p>
	<div class="panel panel-default">
		<div class="panel-heading"> Results</div>
		<div class="panel-body">
			<table>
				<% if (result.length) { %>

					<% result.forEach(function(item){ %>
						<tr>
							<td>
								<!--<form role="addLink" action="/toggleShow" method="post">
									<input type="hidden" name="blogpost_id" value=<%= item["BLOG_POST_ID"] %> >
									<input type="hidden" name="show" value=<%= !item["SHOW"] %> >
									<button class="btn btn-primary" type="submit">Set to <%= !item["SHOW"] %></button>   
								</form>-->
							</td>											
						<% for(var columnName in item) {%>
							<td> <%= item[columnName] %></td>
						<% } %>
							
						</tr>
					<% }) %>
				<% } %>
			</table>
		</div>
	</div>
	</div> <!-- /container -->
</body


</html>